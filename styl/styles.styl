h1
	font-size: 52px;
	font-family: "PlayfairDisplayBlack", serif;
	color: rgb(161, 144, 97);
	line-height: 1
	margin 25px 0
	width: 700px
h3
	font-size: 44px;
	font-family: "PlayfairDisplayBlack", serif;
	color: rgb(23, 22, 18);
	line-height: 0.75;
	text-align: center
	position relative
	&:after
		content: url('..img/divider_black.png')
		display block
		margin 10px auto 60px
.video_wrapper
	margin 0 auto
	position relative
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #fff
		border-radius 3px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
@import 'parts/main.styl'